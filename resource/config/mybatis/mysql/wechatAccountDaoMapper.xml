<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smile.wechat.dao.WechatAccountDao">
	
	<select id="getWechatAccountList" resultType="com.smile.wechat.model.pojo.WechatAccount">
		select * from wechatmanage.wx_account
	</select>
	
	<select id="getWechatAccountById" resultType="com.smile.wechat.model.pojo.WechatAccount">
		select * from wechatmanage.wx_account where id=#{id}
	</select>
	
</mapper>